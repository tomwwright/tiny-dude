{
    "name": "tiny-dude",
    "version": "0.0.1",
    "description": "Tiny Dude Computer, built with React and MobX.",
    "scripts": {
      "compile": "tsc",
      "build": "rm -rf build/* && yarn compile && webpack --config webpack.config.js",
      "test:models": "yarn compile && NODE_PATH=\"$NODE_PATH:$PWD/build/es5\" mocha --recursive build/es5/models/**/*.test.js"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/tomwwright/tiny-dude"
    },
    "author": "Tom Wright",
    "license": "MIT",
    "homepage": "https://github.com/tomwwright/tiny-dude#readme",
    "dependencies": {
      "glob": "^7.1.1",
      "mobx": "^3.2.1",
      "mobx-react": "^4.2.2",
      "react": "^15.3.1",
      "react-dom": "^15.3.1",
      "rebass": "^1.0.4",
      "underscore": "^1.8.3"
    },
    "devDependencies": {
      "@types/chai": "^3.5.2",
      "@types/node": "^7.0.18",
      "@types/react": "^15.0.24",
      "@types/react-dom": "^15.5.0",
      "@types/sinon": "^2.2.1",
      "babel-cli": "^6.24.1",
      "babel-core": "^6.14.0",
      "babel-loader": "^7.0.0",
      "babel-polyfill": "^6.26.0",
      "babel-preset-es2015": "^6.14.0",
      "babel-preset-react": "^6.11.1",
      "chai": "^3.5.0",
      "mocha": "^3.1.2",
      "sinon": "^2.2.0",
      "source-map-loader": "^0.2.1",
      "typescript": "^2.3.4",
      "webpack": "^2.5.1"
    },
    "babel": {
      "presets": [
        "es2015",
        "react"
      ]
    },
    "prettier": {
      "singleQuote": true,
      "trailingComma": "es5",
      "printWidth": 120,
      "tabWidth": 2
    }
  }
  